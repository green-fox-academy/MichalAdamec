@model BankOfSimba.ViewModel.ListOfAnimals
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>List of accounts</title>
    <style>
        table, th, td {
            border: 1px solid black;
        }
    </style>
</head>
<body>
    <table>
        <tr>
            <th>Name</th>
            <th>Ballance</th>
            <th>Type of animal</th>
        </tr>
   @foreach (var animal in @Model.listFromViewModel)            //model je to co posílám z controlleru; View na Models přímo nevidí
    {
        <tr>
            <td>@animal.Name</td>
            <td>@animal.Ballance</td>
            <td>@animal.AnimalType</td>
        </tr>
    }
    </table>
</body>
</html>